{
  "name": "@keepmhwn/mark-over",
  "version": "0.0.1",
  "description": "Transform text within markup tags (HTML) via callbacks while preserving structure",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "html",
    "xml",
    "markup",
    "text",
    "transform",
    "replace",
    "map",
    "i18n",
    "internationalization",
    "translation",
    "parser",
    "node",
    "browser"
  ],
  "author": {
    "name": "minhwan(keepmhwn)",
    "email": "keepmhwn@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/keepmhwn/mark-over"
  },
  "bugs": {
    "url": "https://github.com/keepmhwn/mark-over/issues"
  },
  "homepage": "https://github.com/keepmhwn/mark-over#readme",
  "devDependencies": {
    "@types/jsdom": "^21.1.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "dependencies": {
    "jsdom": "^24.0.0"
  }
}
